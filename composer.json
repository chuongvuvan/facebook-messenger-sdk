{
    "name": "nguyenanhung/facebook-messenger-sdk",
    "description": " Facebook Messenger SDK",
    "homepage": "https://github.com/nguyenanhung/facebook-messenger-sdk",
    "type": "library",
    "keywords": [
        "debug",
        "logger",
        "log"
    ],
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Nguyen An Hung",
            "email": "dev@nguyenanhung.com",
            "homepage": "https://nguyenanhung.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-fileinfo": "*"
    },
    "require-dev": {
        "kint-php/kint": "^3.0",
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload": {
        "psr-4": {
            "nguyenanhung\\FacebookMessenger\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}